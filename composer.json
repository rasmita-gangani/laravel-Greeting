{
  "name": "rasmita-gangani/greeting",
  "description": "A simple Laravel package to provide greetings.",
  "type": "library",
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "rasmita-gangani\\Greeting\\": "packages/rasmita-gangani/Greeting/src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "rasmita-gangani\\Greeting\\GreetingServiceProvider"
      ],
      "aliases": {
        "Greeting": "rasmita-gangani\\Greeting\\Facades\\Greeting"
      }
    }
  },
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^8.0|^9.0|^10.0"
  }
}
